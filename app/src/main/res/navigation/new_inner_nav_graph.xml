<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/LandMapFragment">

    <fragment
        android:id="@+id/LandMapFragment"
        android:name="com.chillarcards.britto.ui.register.LandMapFragment"
        android:label="MobileFragment"
        tools:layout="@layout/fragment_map">
        <action
            android:id="@+id/action_mapFragment_to_landFragment"
            app:destination="@id/CommonHomeFragment" />
        <argument
            android:name="seleId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="isVerifyTrm"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="mobileNo"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/CommonHomeFragment"
        android:name="com.chillarcards.britto.ui.landing.CommonHomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_common_home">

        <action
            android:id="@+id/action_commonFragment_to_mobileFragment"
            app:destination="@id/mobileFragment" />
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="id"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/mobileFragment"
        android:name="com.chillarcards.britto.ui.register.MobileFragment"
        android:label="MobileFragment"
        tools:layout="@layout/fragment_mobile">
        <action
            android:id="@+id/action_mobileFragment_to_OTPFragment"
            app:destination="@id/OTPFragment" />
        <argument
            android:name="seleId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="isVerifyTrm"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="mobileNo"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/OTPFragment"
        android:name="com.chillarcards.britto.ui.register.OTPFragment"
        android:label="OTPFragment"
        tools:layout="@layout/fragment_otp">
        <action
            android:id="@+id/action_oTPFragment_to_generalFragment"
            app:destination="@id/GeneralHomeFragment" />
        <action
            android:id="@+id/action_MapFragment_to_SavedFragment"
            app:destination="@id/AddFragment" />
        <action
            android:id="@+id/action_oTPFragment_to_homeFragment"
            app:destination="@id/homeBaseFragment"
            app:popUpTo="@id/action_oTPFragment_to_homeFragment"
            app:popUpToInclusive="true" />


        <action
            android:id="@+id/action_genhomeFragment_to_bphomeFragment"
            app:destination="@id/bphomeBaseFragment"
            app:popUpTo="@id/action_genhomeFragment_to_bphomeFragment"
            app:popUpToInclusive="true" />

        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="seleId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="otp"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/DoctorFragment"
        android:name="com.chillarcards.britto.ui.register.business.DoctorFragment"
        android:label="DoctorFragment"
        tools:layout="@layout/fragment_add_doctor">
        <action
            android:id="@+id/action_doctorFragment_to_workFragment"
            app:destination="@id/docWorkFragment" />

        <argument
            android:name="type"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="seleId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/RegisterFragment"
        android:name="com.chillarcards.britto.ui.register.business.RegisterFragment"
        android:label="RegisterFragment"
        tools:layout="@layout/fragment_register">

        <action
            android:id="@+id/action_RegisterFragment_to_bphomeFragment"
            app:destination="@id/bphomeBaseFragment"
            app:popUpTo="@id/action_RegisterFragment_to_bphomeFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_regiFragment_to_photoFragment"
            app:destination="@id/PhotoFragment" />

        <argument
            android:name="type"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="seleId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/PhotoFragment"
        android:name="com.chillarcards.britto.ui.register.business.PhotoFragment"
        android:label="PhotoFragment"
        tools:layout="@layout/fragment_register_profile">

        <action
            android:id="@+id/action_photoFragment_to_workFragment"
            app:destination="@id/docWorkFragment" />

        <argument
            android:name="type"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="seleId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/workFragment"
        android:name="com.chillarcards.britto.ui.register.business.WorkHoursFragment"
        android:label="PhotoFragment"
        tools:layout="@layout/fragment_work_hours">

        <argument
            android:name="type"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="seleId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>
  <fragment
        android:id="@+id/docWorkFragment"
        android:name="com.chillarcards.britto.ui.register.business.DocWorkHoursFragment"
        android:label="PhotoFragment"
        tools:layout="@layout/fragment_work_hours">
      <action
          android:id="@+id/action_genhomeFragment_to_bphomeFragment"
          app:destination="@id/bphomeBaseFragment"
          app:popUpTo="@id/action_genhomeFragment_to_bphomeFragment"
          app:popUpToInclusive="true" />
        <argument
            android:name="type"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="seleId"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/AddFragment"
        android:name="com.chillarcards.britto.ui.register.customer.AddLocationFragment"
        android:label="AddFragment"
        tools:layout="@layout/fragment_add_location">
        <action
            android:id="@+id/action_saveFragment_to_homeFragment"
            app:destination="@id/homeBaseFragment"
            app:popUpTo="@id/action_saveFragment_to_homeFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="mobile"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/GeneralHomeFragment"
        android:name="com.chillarcards.britto.ui.register.business.GeneralHomeFragment"
        android:label="GeneralHomeFragment"
        tools:layout="@layout/fragment_general_home">
        <action
            android:id="@+id/action_gnerlFragment_to_registerFragment"
            app:destination="@id/RegisterFragment" />

        <action
            android:id="@+id/action_gnerlFragment_to_doctorFragment"
            app:destination="@id/DoctorFragment" />


        <argument
            android:name="type"
            android:defaultValue=""
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/homeBaseFragment"
        android:name="com.chillarcards.britto.ui.home.HomeBaseFragment"
        android:label="HomeBaseFragment"
        tools:layout="@layout/fragment_home_base" >

    </fragment>

    <fragment
        android:id="@+id/bphomeBaseFragment"
        android:name="com.chillarcards.britto.ui.home.BPHomeBaseFragment"
        android:label="BPhomeBaseFragment"
        tools:layout="@layout/fragment_home_bp_base" >
    </fragment>

</navigation>